{
  "name": "react-native-swipeable-button",
  "version": "1.0.0",
  "description": "A customizable swipeable button for React Native applications",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build"
  },
  "keywords": [
    "react native swipeable button",
    "react native swipeable",
    "react-native slider button",
    "react native slider",
    "react-native slide",
    "react-native",
    "ios",
    "android",
    "swipe",
    "button",
    "unlock",
    "slider"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevelopOnStacks/react-native-swipeable-button.git"
  },
  "author": "Abdur Rahman <personal.abdurrahman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevelopOnStacks/react-native-swipeable-button/issues"
  },
  "homepage": "https://github.com/DevelopOnStacks/react-native-swipeable-button#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "pod-install": "^0.1.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^5.0.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@commitlint/config-conventional": "^17.0.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.1",
    "prettier": "^2.0.5",
    "release-it": "^15.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
